{
  "name": "tourze/credit-merge-bundle",
  "description": "小额流水合并模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/orm": "^3.0",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^6.4",
    "symfony/console": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "tourze/credit-bundle": "0.0.*",
    "tourze/bundle-dependency": "0.0.*",
    "tourze/enum-extra": "~0.0.5",
    "tourze/symfony-cron-job-bundle": "0.1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "CreditMergeBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CreditMergeBundle\\Tests\\": "tests"
    }
  }
